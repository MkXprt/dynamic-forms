{
  "name": "@dynamic-forms/common",
  "version": "0.0.1",
  "author": "Alexander Gebuhr <alexandergebuhr@outlook.com>",
  "description": "DynamicForms is an Angular project for dynamic forms based on JSON.",
  "keywords": [
    "angular",
    "forms",
    "reactive forms",
    "json",
    "json forms",
    "dynamic-forms"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://alexandergebuhr@dev.azure.com/alexandergebuhr/dynamic-forms/_git/dynamic-forms"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:core": "ng build dynamic-forms-core",
    "build:bootstrap": "ng build dynamic-forms-bootstrap",
    "build:clarity": "ng build dynamic-forms-clarity",
    "build:material": "ng build dynamic-forms-material",
    "build:libs": "npm run build:core && npm run build:bootstrap && npm run build:clarity && npm run build:material",
    "build:demo": "ng build dynamic-forms-demo",
    "build:demo:prod": "ng build dynamic-forms-demo --prod --aot",
    "test:core": "ng test dynamic-forms-core --browsers=ChromeHeadless --watch=false --code-coverage",
    "test:bootstrap": "ng test dynamic-forms-bootstrap --browsers=ChromeHeadless --watch=false --code-coverage",
    "test:clarity": "ng test dynamic-forms-clarity --browsers=ChromeHeadless --watch=false --code-coverage",
    "test:material": "ng test dynamic-forms-material --browsers=ChromeHeadless --watch=false --code-coverage",
    "test:libs": "npm run test:core && npm run test:bootstrap && npm run test:clarity && npm run test:material",
    "test:demo": "ng test dynamic-forms-demo --browsers=ChromeHeadless --watch=false",
    "serve:core": "ng build dynamic-forms-core --watch",
    "serve:bootstrap": "ng build dynamic-forms-bootstrap --watch",
    "serve:clarity": "ng build dynamic-forms-clarity --watch",
    "serve:material": "ng build dynamic-forms-material --watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.9",
    "@angular/cdk": "~8.0.2",
    "@angular/common": "~8.2.9",
    "@angular/compiler": "~8.2.9",
    "@angular/core": "~8.2.9",
    "@angular/forms": "~8.2.9",
    "@angular/material": "~8.0.2",
    "@angular/platform-browser": "~8.2.9",
    "@angular/platform-browser-dynamic": "~8.2.9",
    "@angular/router": "~8.2.9",
    "@clr/angular": "^2.1.1",
    "@clr/icons": "^2.1.1",
    "@clr/ui": "^2.1.1",
    "@ngxs/store": "^3.5.0",
    "@webcomponents/custom-elements": "^1.0.0",
    "bootstrap": "^4.3.1",
    "rxjs": "~6.5.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.8",
    "@angular-devkit/build-ng-packagr": "~0.803.8",
    "@angular/cli": "~8.3.8",
    "@angular/compiler-cli": "~8.2.9",
    "@angular/language-service": "~8.2.9",
    "@types/node": "~12.7.11",
    "@types/jasmine": "~3.4.2",
    "@types/jasminewd2": "~2.0.8",
    "codelyzer": "~5.1.2",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.3.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^5.5.1",
    "protractor": "~5.4.2",
    "ts-node": "~8.4.1",
    "tsickle": ">=0.37.0",
    "tslint": "~5.20.0",
    "typescript": "~3.5.3"
  }
}
