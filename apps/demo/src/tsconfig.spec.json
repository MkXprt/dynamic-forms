{
  "extends": "../../../tsconfig.spec.json",
  "compilerOptions": {
    "outDir": "../../../out-tsc/spec",
    "types": [
      "jasmine",
      "node"
    ],
    "paths": {
      "@dynamic-forms/core": [
        "dist/@dynamic-forms/core"
      ],
      "@dynamic-forms/core/*": [
        "dist/@dynamic-forms/core/*"
      ],
      "@dynamic-forms/bootstrap": [
        "dist/@dynamic-forms/bootstrap"
      ],
      "@dynamic-forms/bootstrap/*": [
        "dist/@dynamic-forms/bootstrap/*"
      ],
      "@dynamic-forms/clarity": [
        "dist/@dynamic-forms/clarity"
      ],
      "@dynamic-forms/clarity/*": [
        "dist/@dynamic-forms/clarity/*"
      ],
      "@dynamic-forms/material": [
        "dist/@dynamic-forms/material"
      ],
      "@dynamic-forms/material/*": [
        "dist/@dynamic-forms/material/*"
      ]
    }
  },
  "files": [
    "test.ts",
    "polyfills.ts"
  ],
  "include": [
    "**/*.spec.ts",
    "**/*.d.ts"
  ]
}
